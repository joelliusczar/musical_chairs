  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "python": "${command:python.interpreterPath}",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "preLaunchTask": "Set unit test env",
      "justMyCode": false,
      "env": {
        "PTHONPATH": "${workspaceFolder}/src/musical_chairs_libs"
      }
    },
    {
      "name": "Python: API",
      "type": "python",
      "python": "${command:python.interpreterPath}",
      "request": "launch",
      "program": "${workspaceFolder}/src/api/index.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PTHONPATH": "${workspaceFolder}/src/musical_chairs_libs"
      },
      "args": [
        "${userHome}/.ssh/musical_chairs_localhost_debug.private.key.pem",
        "${userHome}/.ssh/musical_chairs_localhost_debug.public.key.crt"
      ]
    },
    {
      "name": "Python: nginx API",
      "type": "python",
      "python": "${command:python.interpreterPath}",
      "request": "launch",
      "program": "${workspaceFolder}/src/api/index.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PTHONPATH": "${workspaceFolder}/src/musical_chairs_libs"
      }
    },
    {
      "name": "Debug Unit Test",
      "type": "python",
      "preLaunchTask": "Set unit test env",
      "request": "test",
      "justMyCode": true,
  },
  ]
}