{
  "editor.tabSize": 2,
  "editor.rulers": [80, 80],
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#ef40ff"
  },
  "python.analysis.extraPaths": ["src/tests"],
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.indexing": true,
  "python.analysis.useLibraryCodeForTypes": true,
  "python.defaultInterpreterPath": "${workspaceFolder}/test_trash/musical_chairs_dir/mc_env/bin/python",
  "terminal.integrated.env.osx": {
    "PTHONPATH": "${workspaceFolder}/src: ${workspaceFolder}/src/api",
    "lib_name": "musical_chairs_libs",
    "app_name": "musical_chairs_app",
    "test_root": "${workspaceFolder}/test_trash"
  },
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.testing.cwd": "${workspaceFolder}/src/tests",
  "python.testing.pytestPath": "${workspaceFolder}/test_trash/musical_chairs_dir/mc_env/bin/pytest",
  "python.testing.pytestArgs": [
    "-s"
  ],
  "python.terminal.activateEnvironment": false,
  "python.envFile": "${workspaceFolder}/test_trash/musical_chairs_dir/config/.env",
  "search.exclude": {
    "**/test_trash": true
  },
  "editor.insertSpaces": false,
}